<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup>
    <OutDir>$(ProjectName)-$(Configuration)\</OutDir>
    <IntDir>$(ProjectName)-$(Configuration)-OBJ\</IntDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <PostBuildEvent>
      <Command>echo 正在执行自定义命令 &amp;&amp; set url="$(ProjectName)-$(Configuration)\$(ProjectName).lib" &amp;&amp; set tourl="$(SolutionDir)..\res\SDK_$(Configuration).lib" &amp;&amp; echo 复制 $(ProjectName).lib -^&gt; \res\SDK_$(Configuration).lib &amp;&amp; copy %url% %tourl% &gt;nul &amp;&amp; echo 自定义命令执行完成 </Command>
    </PostBuildEvent>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>